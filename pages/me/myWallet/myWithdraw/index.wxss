/* pages/myWithdraw/index.wxss */
page{
  background-color:#F1F1F1;
  padding-top:20rpx;
}
.withdraw-item{
  width:100%;
  height:146rpx;
  padding:30rpx 14.5px;
  box-sizing:border-box;
  background-color: #fff;
  border-bottom:1px solid #E3E3E3;
  position:relative;
}
.withdraw-title{
  height:40rpx;
  line-height:40rpx;
  font-size:34rpx;
  color:#343434;
}
.withdraw-date{
  height:40rpx;
  line-height:40rpx;
  font-size:30rpx;
  color:#979797;
  margin-top:6rpx;
}
.withdraw-num{
  position:absolute;
  right:14.5px;
  top:20rpx;
  font-size:44rpx;
  color:#FFA201;
}
.withdraw-status{
  position:absolute;
  right:14.5px;
  bottom:26rpx;
  font-size:30rpx;
  color:#9E9E9E;
}
::-webkit-scrollbar{
  width:0;
  height:0;
  color:trasparent;
}