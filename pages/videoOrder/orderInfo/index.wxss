@import "../../template/index.wxss";
.videoplayer{
  position:fixed;
  width:100%;
  height:100%;
  z-index:9999;
}

.closeVideo{
  width:80rpx;
  height:80rpx;
  line-height:80rpx;
  font-size:40rpx;
  color:#fff;
  background-color: #99999B;
  position:absolute;
  top:100rpx;
  left:100rpx;
  z-index:10000;
  border-radius:50%;
  opacity:0.3;
}

.videoImage-box{
  width: 120rpx;
  height: 150rpx;
  position:relative;
}

.play{
  margin-left:-10rpx;
}

